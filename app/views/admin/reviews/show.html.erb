<%= provide :title, t(".title") %>
<div id="page-content-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-lg-offset-2 show-review">
        <h1 class="rv-title"> <%= @review.title %> </h1>
        <h4> <%= @review.user.full_name %> </h4>
        <br />
        <p class="rv-content"> <%= @review.content %> </p>
        <button class="btn btn-primary"><%= t ".edit" %></button>
        <%= link_to t(".all_review"), admin_reviews_path %>
      </div>
    </div>
  </div>
</div>
