<% provide :title, t(".header") %>
<div id="page-content-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-lg-offset-2">
        <% flash.each do |message_type, message| %>
          <div class="alert alert-<%= message_type %>"><%= message %></div>
        <% end %>
        <h1><%= t ".title" %></h1>
        <p><%= t ".content" %></p>
      </div>
    </div>
  </div>
</div>
